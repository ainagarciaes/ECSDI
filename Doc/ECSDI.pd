    <field name="diagramSettings"><list>
	<object type="DiagramSettings" id="2">
	    <field name="diagramId">
		<object type="DiagramId" id="3">
		    <field name="stringId">Goal Overview</field>
		</object>
	    </field>
	    <field name="entitySettings"><list>
		<object type="DiagramEntitySetting" id="4">
		    <field name="entity">
			<object type="Goal" id="5">
			    <base type="Entity">
				<field name="name">Organitzar viatge</field>
				<field name="description">L'objectiu principal del sistema és conseguir donar al usuari un viatge organitzat, amb tots els elements que el conformen.</field>
				<field name="uniqueId">1</field>
			    </base>

			    <field name="subGoals"><list>
				<object type="Goal" id="6">
				    <base type="Entity">
					<field name="name">Escollir allotjament i transport</field>
					<field name="description">El primer objectiu per tal de organitzar un viatge és tenir fixat un lloc d'allotjament adaptat a les preferencies del usuari i mitjans de transport per anar/tornar entre els dies escollits.</field>
					<field name="uniqueId">2</field>
				    </base>

				    <field name="subGoals"><list>
					<object type="Goal" id="7">
					    <base type="Entity">
						<field name="name">Obtenir restriccions</field>
						<field name="description">El sistema ha de ser capaç d'obtenir les restriccions de l'usuari per tal d'ajustar el viatge a les seves necessitats. En aquest punt, les restriccions necessaries serien les dates, el lloc on vol anar, ci vol un apartament cèntric i el pressupost.</field>
						<field name="uniqueId">3</field>
					    </base>

					    <field name="subGoals"><list>
						<object type="Goal" id="8">
						    <base type="Entity">
							<field name="name">Obtenir dates</field>
							<field name="description">Cal preguntar al usuari quines dates orientatives vol per dur a terme el viatge + lloc.</field>
							<field name="uniqueId">4</field>
						    </base>

						</object>

						<object type="Goal" id="9">
						    <base type="Entity">
							<field name="name">Obtenir pressupost</field>
							<field name="description">Cal preguntar al usuari quin es el pressupost que està disponible per el transport i l'allotjament.</field>
							<field name="uniqueId">5</field>
						    </base>

						</object>

						<object type="Goal" id="10">
						    <base type="Entity">
							<field name="name">Obtenir localitzacio</field>
							<field name="description">Cal preguntar a l'usuari la ubicacio de l'allotjament (centric, no centric)</field>
							<field name="uniqueId">6</field>
						    </base>

						</object>
						</list>
					    </field>
					</object>

					<object type="Goal" id="11">
					    <base type="Entity">
						<field name="name">Buscar proveïdors d'informació</field>
						<field name="description">S'ha de poder accedir a serveis externs per tal de buscar informació sobre possibilitats d'allotjament i transport segons les restriccions de l'usuari.</field>
						<field name="uniqueId">7</field>
					    </base>

					    <field name="subGoals"><list>
						<object type="Goal" id="12">
						    <base type="Entity">
							<field name="name">Enviar restriccions</field>
							<field name="description">Enviar les restriccions de l'usuari per tal d'obtenir resultats ajustats a les restriccions donades.</field>
							<field name="uniqueId">8</field>
						    </base>

						</object>

						<object type="Goal" id="13">
						    <base type="Entity">
							<field name="name">Enviar preferencies</field>
							<field name="description">Enviar les preferencies per tal d'obtenir resultats mes personalitzats.</field>
							<field name="uniqueId">13</field>
						    </base>

						</object>

						<object type="Goal" id="14">
						    <base type="Entity">
							<field name="name">Obtenir possibilitats</field>
							<field name="description">Obtenir les possibilitats d'allotjaments i mitjans de transport per tal de processar-les i pendre una decisió posteriorment.</field>
							<field name="uniqueId">14</field>
						    </base>

						</object>
						</list>
					    </field>
					</object>

					<object type="Goal" id="15">
					    <base type="Entity">
						<field name="name">Obtenir preferencies</field>
						<field name="description">Obtenir preferencies del usuari per tal de poder obtenir resultats mes personalitzats segons els gustos i historial del usuari.</field>
						<field name="uniqueId">9</field>
					    </base>

					    <field name="subGoals"><list>
						<object type="Goal" id="16">
						    <base type="Entity">
							<field name="name">Obtenir historial usuari</field>
							<field name="description">Preferencies obtingudes basant-se en experiencies passades de l'usuari amb el sistema.</field>
							<field name="uniqueId">10</field>
						    </base>

						</object>

						<object type="Goal" id="17">
						    <base type="Entity">
							<field name="name">Obtenir valoracions</field>
							<field name="description">Obtenir preferencies del usuari basant-se en les millors valoracions que ha donat l'usuari al sistema.</field>
							<field name="uniqueId">11</field>
						    </base>

						</object>

						<object type="Goal" id="18">
						    <base type="Entity">
							<field name="name">Obtenir informació d'usuaris similars</field>
							<field name="uniqueId">12</field>
						    </base>

						</object>
						</list>
					    </field>
					</object>

					<object type="Goal" id="19">
					    <base type="Entity">
						<field name="name">Triar alternativa</field>
						<field name="description">Un cop tenim totes les possiblitats, cal que el sistema les analitzi i esculli la millor opció per l'usuari concret.</field>
						<field name="uniqueId">15</field>
					    </base>

					    <field name="subGoals"><list>
						<object type="Goal" id="20">
						    <base type="Entity">
							<field name="name">Analitzar possibilitats</field>
							<field name="description">El sistema ha de ser capaç d'analitzar un conjunt d'alternatives i escollir la que millor s'adapti al profiling del usuari.</field>
							<field name="uniqueId">16</field>
						    </base>

						</object>

						<object type="Goal" id="21">
						    <base type="Entity">
							<field name="name">Decidir transport</field>
							<field name="description">El sistema ha de ser capaç d'escollir un mitja de transport amb les dades analitzades.</field>
							<field name="uniqueId">17</field>
						    </base>

						</object>

						<object type="Goal" id="22">
						    <base type="Entity">
							<field name="name">Decidir allotjament</field>
							<field name="description">El sistema ha de ser capaç d'escollir un allotjament amb les dades analitzades.</field>
							<field name="uniqueId">18</field>
						    </base>

						</object>
						</list>
					    </field>
					</object>
					</list>
				    </field>
				</object>

				<object type="Goal" id="23">
				    <base type="Entity">
					<field name="name">Escollir visites</field>
					<field name="description">El sistema s'ha d'encarregar de organitzar un horari amb llocs a visitar escollits segons les preferencies de l'usuari.</field>
					<field name="uniqueId">19</field>
				    </base>

				    <field name="subGoals"><list>
					<object type="Goal" id="24">
					    <base type="Entity">
						<field name="name">Obtenir prioritats</field>
						<field name="description">S'ha de saber quin tipus de viatge vol l'usuari i els gustos i preferencies que aquest té.</field>
						<field name="uniqueId">22</field>
					    </base>

					    <field name="subGoals"><list>
						<object ref="15"/>

						<object type="Goal" id="25">
						    <base type="Entity">
							<field name="name">Obtenir dies</field>
							<field name="description">El sistema ha de saber amb exactitud quines son les dates i hores escollides finalment per dur a terme el viatge. (no dels requeriments de l'usuari sino del allotjament i transport escollits)</field>
							<field name="uniqueId">23</field>
						    </base>

						</object>

						<object type="Goal" id="26">
						    <base type="Entity">
							<field name="name">Obtenir preferencies tipu viatge</field>
							<field name="description">Cal obtenir de l'usuari les ponderacions del tipus de viatge que vol (si el vol ludic, cultural, etc.)</field>
							<field name="uniqueId">24</field>
						    </base>

						</object>
						</list>
					    </field>
					</object>
					</list>
				    </field>
				</object>

				<object type="Goal" id="27">
				    <base type="Entity">
					<field name="name">Confirmar viatge</field>
					<field name="description">El sistema ha de ser capaç d'informar a l'usuari de les decisions preses i donar-li la possibilitat de confirmar-lo si el vol realitzar abans de procedir a les reserves i pagament d'aquest.</field>
					<field name="uniqueId">20</field>
				    </base>

				</object>

				<object type="Goal" id="28">
				    <base type="Entity">
					<field name="name">Gestionar pagaments</field>
					<field name="description">El sistema ha de ser capaç de gestionar pagaments, ja sigui per transferencia com de forma automàtica online.</field>
					<field name="uniqueId">21</field>
				    </base>

				</object>
				</list>
			    </field>
			</object>
